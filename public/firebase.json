{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/sitemap",
        "destination": "/sitemap.xml"
      }
    ],


    "cleanUrls": true,

    "trailingSlash": false

  }
}
